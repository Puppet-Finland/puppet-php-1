{
  "name": "bodgit-php",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing PHP",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-php",
  "project_page": "https://github.com/bodgit/puppet-php",
  "issues_url": "https://github.com/bodgit/puppet-php/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.6.0 <5.0.0"
    }
  ],
  "tags": [
    "php",
    "fpm"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.13.0 <5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">=1.6.0 <3.0.0"
    },
    {
      "name": "thrnio/ip",
      "version_requirement": ">=1.0.0 <2.0.0"
    },
    {
      "name": "bodgit/bodgitlib",
      "version_requirement": ">=1.6.0 <3.0.0"
    }
  ]
}
